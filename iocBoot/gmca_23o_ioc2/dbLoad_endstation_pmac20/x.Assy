file "$(TPMAC)/xDb/X_Calibrate.db"
{
        pattern { assy, d1 }
# pmac20:
        { 23o:GO:Om:,  23o:GO:OM:   }
        { 23o:JET:Ps:, 23o:JET:P:   }
        { 23o:TV:V1:,  23o:TV:ZM1:  }
	{ 23o:TV:V2:,  23o:TV:ZM2:  }
}
#----------------------------------------------------------------------------
