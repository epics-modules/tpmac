#----------------------------------------------------------------------------
file "$(TPMAC)/xDb/X_Tsub.db"
{
	pattern { assy, d1, m1, val, nla, tsub }
# tsub's are C programs implementing conversion between drives <-> motors <-> axes.
# nla=0/1 correspond to absolute/relative conversion.
# pmac20:
        { 23o:GO:Om:,  23o:GO:OM:,  23o:GO:mom:, RqsPos, 0, tsubXPs }
        { 23o:GO:Om:,  23o:GO:OM:,  23o:GO:mom:, RqsBkl, 1, tsubXPs }
        { 23o:JET:Ps:, 23o:JET:P:,  23o:JET:mp:, RqsPos, 0, tsubXPs }
        { 23o:JET:Ps:, 23o:JET:P:,  23o:JET:mp:, RqsBkl, 1, tsubXPs }
        { 23o:TV:V1:,  23o:TV:ZM1:, 23o:TV:mz1:, RqsPos, 0, tsubXPs }
        { 23o:TV:V1:,  23o:TV:ZM1:, 23o:TV:mz1:, RqsBkl, 1, tsubXPs }
	{ 23o:TV:V2:,  23o:TV:ZM2:, 23o:TV:mz2:, RqsPos, 0, tsubXPs }
	{ 23o:TV:V2:,  23o:TV:ZM2:, 23o:TV:mz2:, RqsBkl, 1, tsubXPs }
}
#----------------------------------------------------------------------------
file "$(TPMAC)/xDb/X_TsubRbk.db"
{
	pattern { assy, d1, desc_d1, egu_d1, m1, val, nla, tsub }
# pmac20:
        { 23o:GO:Om:,  23o:GO:OM:,  "gonio omega",     "(mm)",   23o:GO:mom:, ActPos, 0, tsubXPs }
        { 23o:JET:Ps:, 23o:JET:P:,  "cryo position",   "(mm)",   23o:JET:mp:, ActPos, 0, tsubXPs }
        { 23o:TV:V1:,  23o:TV:ZM1:, "hi-res. zoom",    "(mm)",   23o:TV:mz1:, ActPos, 0, tsubXPs }
	{ 23o:TV:V2:,  23o:TV:ZM2:, "hi-res. zoom",    "(mm)",   23o:TV:mz2:, ActPos, 0, tsubXPs }
}
#----------------------------------------------------------------------------
file "$(TPMAC)/xDb/X_TsubSpeed.db"
{
  pattern { assy, d1, egu_d1, m1, tsub }
# pmac20:
        { 23o:GO:Om:,  23o:GO:OM:,  "(degr/s)", 23o:GO:mom:, tsubXPs }
        { 23o:JET:Ps:, 23o:JET:P:,  "(mm/s)",   23o:JET:mp:, tsubXPs }
        { 23o:TV:V1:,  23o:TV:ZM1:, "(mm/s)",   23o:TV:mz1:, tsubXPs }
        { 23o:TV:V2:,  23o:TV:ZM2:, "(mm/s)",   23o:TV:mz2:, tsubXPs }
}
