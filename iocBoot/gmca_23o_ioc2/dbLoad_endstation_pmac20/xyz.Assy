#----------------------------------------------------------------------------
file "$(TPMAC)/xyzDb/XYZ_Tsub.db"
{
 	pattern { assy, d1, d2, d3, m1, m2, m3, val, nla, tsub }
# tsub's are C programs implementing conversion between drives <-> motors <-> axes.
# nla=0/1 correspond to absolute/relative conversion.
# pmac20:
  { 23o:GO:Kp:, 23o:GO:KP:, 23o:GO:PH:, 23o:GO:CZ:, 23o:GO:mkp:, 23o:GO:mph:, 23o:GO:mcz:, RqsPos, 0, tsubXYZPs }
  { 23o:GO:Kp:, 23o:GO:KP:, 23o:GO:PH:, 23o:GO:CZ:, 23o:GO:mkp:, 23o:GO:mph:, 23o:GO:mcz:, RqsBkl, 1, tsubXYZPs }
  { 23o:GO:St:, 23o:GO:SH:, 23o:GO:SW:, 23o:GO:SV:, 23o:GO:msh:, 23o:GO:msw:, 23o:GO:msv:, RqsPos, 0, tsubXYZPs }
  { 23o:GO:St:, 23o:GO:SH:, 23o:GO:SW:, 23o:GO:SV:, 23o:GO:msh:, 23o:GO:msw:, 23o:GO:msv:, RqsBkl, 1, tsubXYZPs }
  { 23o:GS:St:, 23o:GS:YW:, 23o:GS:X:,  23o:GS:PT:, 23o:GS:myw:, 23o:GS:mx:,  23o:GS:mpt:, RqsPos, 0, tsubXYZPs }
  { 23o:GS:St:, 23o:GS:YW:, 23o:GS:X:,  23o:GS:PT:, 23o:GS:myw:, 23o:GS:mx:,  23o:GS:mpt:, RqsBkl, 1, tsubXYZPs }
  { 23o:BS:Ps:, 23o:BS:D:,  23o:BS:PT:, 23o:BS:YW:, 23o:BS:md:,  23o:BS:mpt:, 23o:BS:myw:, RqsPos, 0, tsubXYZPs }
  { 23o:BS:Ps:, 23o:BS:D:,  23o:BS:PT:, 23o:BS:YW:, 23o:BS:md:,  23o:BS:mpt:, 23o:BS:myw:, RqsBkl, 1, tsubXYZPs }
}
