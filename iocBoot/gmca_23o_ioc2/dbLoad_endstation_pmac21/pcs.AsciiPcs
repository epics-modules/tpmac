        { 23o:COL:St:, 1,  1 }
        { 23o:XY:P1:,  1,  2 }
        { 23o:RB:Rd:,  1,  3 }
        { 23o:RB:Rr:,  1,  4 }
        { 23o:FLU:Di:, 1,  5 }
        { 23o:PIN:St:, 1,  6 }
        { 23o:HS:Av:,  1,  7 }
        { 23o:HS:Ah:,  1,  8 }
        { 23o:XY:P2:,  1,  9 }
        { 23o:XY:P3:,  1, 10 }
        { 23o:XY:P4:,  1, 11 }
        { 23o:XY:P5:,  1, 12 }
        { 23o:XY:P6:,  1, 13 }
        { 23o:XY:P7:,  1, 14 }
        { 23o:XY:P8:,  1, 15 }
        { 23o:XY:P9:,  1, 16 }
}
