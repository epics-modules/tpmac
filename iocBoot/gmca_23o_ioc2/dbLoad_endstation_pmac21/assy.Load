#       { 23o:RB:Rr:,  23o:RB:mra:,  1,  4 }
#	{ 23o:FLU:Di:, 23o:FLU:md:,  1,  5 }
#	{ 23o:PIN:St:, 23o:PIN:mp:,  1,  6 }
        { 23o:HS:Av:,  23o:HS:mt:,   23o:HS:mb:,   1,  7 }
        { 23o:HS:Ah:,  23o:HS:mo:,   23o:HS:mi:,   1,  8 }
        { 23o:XY:P2:,  23o:XY:mx2:,  23o:XY:my2:,  1,  9 }
        { 23o:XY:P3:,  23o:XY:mx3:,  23o:XY:my3:,  1, 10 }
        { 23o:XY:P4:,  23o:XY:mx4:,  23o:XY:my4:,  1, 11 }
        { 23o:XY:P5:,  23o:XY:mx5:,  23o:XY:my5:,  1, 12 }
        { 23o:XY:P6:,  23o:XY:mx6:,  23o:XY:my6:,  1, 13 }
        { 23o:XY:P7:,  23o:XY:mx7:,  23o:XY:my7:,  1, 14 }
        { 23o:XY:P8:,  23o:XY:mx8:,  23o:XY:my8:,  1, 15 }
        { 23o:XY:P9:,  23o:XY:mx9:,  23o:XY:my9:,  1, 16 }
}
