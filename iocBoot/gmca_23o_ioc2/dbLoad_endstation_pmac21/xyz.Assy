#----------------------------------------------------------------------------
file "$(TPMAC)/xyzDb/XYZ_Tsub.db"
{
 	pattern { assy, d1, d2, d3, m1, m2, m3, val, nla, tsub }
# tsub's are C programs implementing conversion between drives <-> motors <-> axes.
# nla=0/1 correspond to absolute/relative conversion.
# pmac21:
  { 23o:RB:Rd:, 23o:RB:RX:, 23o:RB:RY:, 23o:RB:RZ:, 23o:RB:mrx:, 23o:RB:mry:, 23o:RB:mrz:, RqsPos, 0, tsubXYZPs }
  { 23o:RB:Rd:, 23o:RB:RX:, 23o:RB:RY:, 23o:RB:RZ:, 23o:RB:mrx:, 23o:RB:mry:, 23o:RB:mrz:, RqsBkl, 1, tsubXYZPs }
}
#----------------------------------------------------------------------------
file "$(TPMAC)/xyzDb/XYZ_TsubRbk.db"
{
 	pattern { assy, d1, desc_d1, egu_d1, d2, desc_d2, egu_d2, d3, desc_d3, egu_d3, m1, m2, m3, val, nla, tsub }
# pmac21:
  { 23o:RB:Rd:, 23o:RB:RX:, "Dewar X", "(mm)", 23o:RB:RY:, "Dewar Y", "(mm)", 23o:RB:RZ:, "Dewar Z", "(mm)", 23o:RB:mrx:, 23o:RB:mry:, 23o:RB:mrz:, ActPos, 0, tsubXYZPs }
}
