file $(TPMAC)/xDb/X_Calibrate.db
{
        pattern { assy, d1 }
# pmac11:
        { 23b:BP:Mo:,  23b:BP:MOZ: }
        { 23b:VF:Be:,  23b:VF:BE: }
        { 23b:BP:Vf:,  23b:BP:VFZ: }
}
#---------------------------------------------------------------------------- 
file $(TPMAC)/xDb/X_LimAmp.db
{
 	pattern { assy, d1 }
# pmac11:
        { 23b:BP:Mo:,  23b:BP:MOZ: }
