#----------------------------------------------------------------------------  
file $(TPMAC)/xyDb/XY_Tsub.db
{
 	pattern { assy, d1, d2, m1, m2, val, nla, tsub }
# tsub's are C programs implementing conversion between drives <-> motors <-> axes.
# nla=0/1 correspond to absolute/relative conversion.  
# pmac10:
  { 23o:MO:Ps:,  23o:MO:Z:,   23o:MO:Y:,   23o:MO:mz:,   23o:MO:my:,   RqsPos, 0, tsubXYPs }
  { 23o:MO:Ps:,  23o:MO:Z:,   23o:MO:Y:,   23o:MO:mz:,   23o:MO:my:,   RqsBkl, 1, tsubXYPs }
  { 23o:MO:Tn2:, 23o:MO:P2:,  23o:MO:R2:,  23o:MO:mp2:,  23o:MO:mr2:,  RqsPos, 0, tsubXYPs }
  { 23o:MO:Tn2:, 23o:MO:P2:,  23o:MO:R2:,  23o:MO:mp2:,  23o:MO:mr2:,  RqsBkl, 1, tsubXYPs }
}
#----------------------------------------------------------------------------  
file $(TPMAC)/xyDb/XY_TsubRbk.db
{
 	pattern { assy, d1, desc_d1, egu_d1, d2, desc_d2, egu_d2, m1, m2, val, nla, tsub }
# pmac10:
  { 23o:MO:Ps:,  23o:MO:Z:,   "Z",               "(mm)",   23o:MO:Y:,  "Y",              "(mm)",   23o:MO:mz:,   23o:MO:my:,   ActPos, 0, tsubXYPs }
  { 23o:MO:Tn2:, 23o:MO:P2:,  "Crystal-2 Pitch", "(mrad)", 23o:MO:R2:, "Crystal-2 Roll", "(mrad)", 23o:MO:mp2:,  23o:MO:mr2:,  ActPos, 0, tsubXYPs }
}
